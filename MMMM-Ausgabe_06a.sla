        <PAGEOBJECT XPOS="751.969133858268" YPOS="3807.57614173228" OwnPage="8" ItemID="2001664890" PTYPE="4" WIDTH="496.0629921" HEIGHT="340.021417322835" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="14.4" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L496.063 0 L496.063 340.021 L0 340.021 L0 0 Z" copath="M0 0 L496.063 0 L496.063 340.021 L0 340.021 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" PSTYLE="Baseline Blocksatz" LAYER="0" NEXTITEM="2001660778" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Baseline Blocksatz"/>
