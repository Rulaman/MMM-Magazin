        <PAGEOBJECT XPOS="142.52031496063" YPOS="16219.6023735907" OwnPage="35" ItemID="196082811" PTYPE="4" WIDTH="496.062992125984" HEIGHT="380.23620908649" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" ANNAME="Kopie von Text337 (23)" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="3" COLGAP="14.4" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L496.063 0 L496.063 380.236 L0 380.236 L0 0 Z" copath="M0 0 L496.063 0 L496.063 380.236 L0 380.236 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" ALIGN="3" LAYER="0" NEXTITEM="196074731" BACKITEM="-1">
