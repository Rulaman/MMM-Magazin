        <PAGEOBJECT OwnPage="127" PTYPE="4" XPOS="142.52031496063" YPOS="56611.4373811434" WIDTH="514.999" HEIGHT="555.31584720315" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="3" COLGAP="14.4" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 514.999 0 514.999 0 514.999 0 514.999 0 514.999 555.316 514.999 555.316 514.999 555.316 514.999 555.316 0 555.316 0 555.316 0 555.316 0 555.316 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 514.999 0 514.999 0 514.999 0 514.999 0 514.999 555.316 514.999 555.316 514.999 555.316 514.999 555.316 0 555.316 0 555.316 0 555.316 0 555.316 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" PSTYLE="Baseline Blocksatz" LAYER="0" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FCOLOR="DeepSkyBlue4" CH="Teil 1: Wie alles be­ginnt...äh…­be­gann"/>
            <para PARENT="Lösung Zwischenüberschrift Azure4"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Nimm in Bernards Zimmer das Kissen und du erhältst eine Feder Öffne beide Schreib­tisch-schubladen und schaue hinein. (Du erhältst jeweils einen  halben Lukenöffner). Benutze die Feder anschließend mit dem Buch der Farben auf Bernards Bücherbrett und danach mit den Hausaufgaben."/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Gehe nach der Zwischensequenz in Britneys Zimmer und benutze eine Hälfte des Lukenöffners mit dem Tampon. Setze danach beide Teile zusammen und benutze den reparierten Lukenöffner mit der Dachboden-Luke. Wähle nun das Indy-Kostüm aus­."/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT FCOLOR="DeepSkyBlue4" CH="Teil 2: Dekoration der Edi­son-Vil­la"/>
            <para PARENT="Lösung Zwischenüberschrift Azure4"/>
            <ITEXT CH="Verlasse nach der Cutscene das Wohnzimmer und gehe in die Küche. Dort nimmst du das Messer mit und schaust in die Spüle, in der du eine Socke findest. Gehe in den Speisesaal und nimm den Kürbis. Benutze anschließend das Messer mit dem Kürbis und du erhältst einen... naja... ausgehöhlten Kürbis halt."/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Gehe nun die Treppe hinauf ins Kunstzimmer und nimm dort Farbeimer und Pinsel, sowie das Bild, das rechts an der Wand hängt. Benutze Pinsel mit Farbeimer. Gehe dann zurück ins Wohnzimmer, stelle den Kürbis auf den kleinen Tisch, bepinsle die Wand über dem Schränkchen und hänge das Bild ganz rechts im Raum neben die Tür zur Bibliothek. So! Fertig de­ko­rier­t!"/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Teil 3: Ich trink Voll­milch"/>
            <para PARENT="Lösung Zwischenüberschrift Azure4"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Betrete den S-Mart und spiele mit dem Spielautomaten (du musst bei dem Minispiel darauf achten, dass du weder mit dem Mauscursor noch mit Bernard den violetten Bereich berührst), gehe bis zur Milch und du hast‘s ge­schafft."/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Teil 4: Finale mit Milchsockensäure"/>
            <para PARENT="Lösung Zwischenüberschrift Azure4"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="Benutze nach der - verdammt langen - Zwischensequenz die Socke mit der Wasserpistole und schütte anschließend die Milch dazu. Gib diese höchst gefährliche Waffe nun an Ed."/>
            <para PARENT="Baseline Blocksatz"/>
            <para PARENT="Baseline Blocksatz"/>
            <ITEXT CH="WeirdEd strikes back - Sonn­tag, 12. De­zem­ber 2010"/>
            <trail PARENT="Baseline Blocksatz: Redakteur"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
